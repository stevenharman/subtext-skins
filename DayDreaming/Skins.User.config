<?xml version="1.0"?>
<SkinTemplates xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <!--
		Note that multiple skins may share the same template folder.
		Each template folder by default should have a style.css file.
		This file does not need to be configured in this section.

		For skins that share a template folder, the skins should be
		distinguished by their stylesheet.  Note that when specifying
		a StyleSheet="" attribute, this style is rendered AFTER "style.css"
		allowing the skin to override template specific styles.
	-->
    <Skins>
        <SkinTemplate Name="CityNight" TemplateFolder="CityNight">
            <Scripts>
                <Script Src="~/scripts/prototype.js" />
                <Script Src="~/scripts/scriptaculous.js?load=effects" />
                <Script Src="~/scripts/lightbox.js" Defer="true" />
                <Script Src="~/scripts/LiveCommentPreview.js" Defer="true" />
                <Script Src="~/scripts/XFNHighlighter.js" Defer="true" />
                <Script Src="Scripts/citynight.js" />
                <Script Src="Scripts/tableEffects.js" />
            </Scripts>
            <Styles>
                <Style href="~/skins/_System/csharp.css" />
                <Style href="~/skins/_System/commonstyle.css" />
                <Style href="~/skins/_System/commonlayout.css" />
                <Style href="~/scripts/XFNHighlighter.css" />
                <Style href="~/scripts/lightbox.css" media="screen"/>
                <Style href="search.css" />
                <Style href="print.css" media="print" />
                <Style href="IEPatches.css" conditional="if lte IE 6" />
                <Style href="IE7Patches.css" conditional="if gte IE 7" />
            </Styles>
        </SkinTemplate>
        <SkinTemplate Name="Day Dreamin'" TemplateFolder="DayDreaming">
            <Scripts>
				<Script Src="~/scripts/prototype.js" />
				<Script Src="~/scripts/scriptaculous.js?load=effects" />
				<Script Src="~/scripts/lightbox.js" />
				<Script Src="~/scripts/LiveCommentPreview.js" />
				<Script Src="~/scripts/XFNHighlighter.js" />
				<Script Src="~/scripts/ExternalLinks.js" />
				<Script Src="scripts/niftycube.js" />
			</Scripts>
			<Styles>
				<Style href="~/skins/_System/csharp.css" />
				<Style href="~/skins/_System/commonstyle.css" />
				<Style href="~/skins/_System/commonlayout.css" />
				<Style href="~/scripts/XFNHighlighter.css" />
				<Style href="~/scripts/lightbox.css" />
				<Style href="niftyCorners.css" />
				<Style href="search.css" />
				<Style href="print.css" media="print" />
				<Style href="IEPatches.css" conditional="if lte IE 6" />
			</Styles>
        </SkinTemplate>
        <SkinTemplate Name="dark" TemplateFolder="dark">
            <Scripts>
                <Script Src="~/scripts/prototype.js" />
                <Script Src="~/scripts/scriptaculous.js?load=effects" />
                <Script Src="~/scripts/lightbox.js" Defer="true" />
                <!--<Script Src="~/scripts/LiveCommentPreview.js" Defer="true" />-->
                <Script Src="~/scripts/XFNHighlighter.js" Defer="true" />
                <Script Src="~/scripts/tableEffects.js" Defer="true" />
            </Scripts>
            <Styles>
                <Style href="~/skins/_System/commonstyle.css" />
                <Style href="~/skins/_System/commonlayout.css" />
                <Style href="~/scripts/XFNHighlighter.css" />
                <Style href="~/scripts/lightbox.css" media="screen"/>
				        <Style href="layout.css" />
				        <Style href="csharp.css" />
                <Style href="print.css" media="print" />
                <Style href="ie-hacks.css" conditional="if ie" />
            </Styles>
        </SkinTemplate>
    </Skins>
</SkinTemplates>
